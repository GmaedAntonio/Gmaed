    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        int x = Integer.parseInt(jTextField1.getText());
        int y = Integer.parseInt(jTextField2.getText());
        
         int sum;
        sum = x + y;
         int diff;
         diff = x - y;
         int prod;
         prod = x * y;
         int qout;
         qout = x / y;
        jLabel4.setText("The sum of 2 numbers;" + String.valueOf(sum));
        jLabel3.setText("The diff of 2 numbers:" + String.valueOf(diff));
        jLabel1.setText("The product of 2 numbers;" + String.valueOf(prod));
        jLabel2.setText("The qout of 2 numbers:" +String.valueOf(qout));
        
        
    }                                        

