    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        int x = Integer.parseInt(textField1.getText());
        int y = Integer.parseInt(textField2.getText());
        int z = Integer.parseInt(textField3.getText());
        int a = Integer.parseInt(textField4.getText());
        int sum;
        sum = x + y + z + a;
        float qout;
        qout = sum / 4;
        textField5.setText(String.valueOf(qout));
    }                                        

